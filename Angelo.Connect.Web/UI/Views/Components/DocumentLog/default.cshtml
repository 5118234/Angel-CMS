@*
@model Angelo.Drive.Models.LogDescriptor

<script src="~/app/modules/components/documentLog.js"></script>

@await Html.PartialAsync("~/UI/Views/Components/DocumentLog/_summary.cshtml", Model.Summary)

<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">Events</h3>
    </div>
    <div class="panel-body">
        <table id="docLog" class="table table-striped table-hover">
            @foreach (var log in Model.Events)
            {
                <tr><td>@log.Created</td><td>@log.Action</td><</tr>
            }
        </table>
    </div>
</div>


<script>
    $(document).ready(function () {
        attachSubsPubEvents_DocLog();
    });
</script>

<script id="logRowTemplate" type="text/x-kendo-tmpl">
    <tr><td>#: action #</td><td>#: created #</td></tr>
</script>
*@